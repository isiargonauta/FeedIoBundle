{
    "name": "debril/feedio-bundle",
    "type": "symfony-bundle",
    "description": "RSS and Atom support for Symfony 2",
    "keywords": ["rss", "atom"],
    "license": "LGPL-3.0",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/alexdebril/FeedIoBundle.git"
        }
    ],
    "replace": {
        "debril/rss-atom-bundle": "1.*"
    },
    "require": {
        "php": ">=5.4",
        "symfony/config": "~2.6",
        "symfony/console": "~2.6",
        "guzzlehttp/guzzle": "~4.1",
        "debril/feed-io": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "doctrine/common": "~2.3",
        "doctrine/doctrine-bundle": "dev-master",
        "symfony/framework-bundle": "~2.6",
        "symfony/process": "~2.6",
        "symfony/validator": "~2.6",
        "symfony/finder": "~2.6",
        "symfony/form": "~2.6",
        "symfony/browser-kit": "~2.6"
    },
    "autoload": {
        "psr-0": {
            "Debril\\FeedIoBundle": ""
        }
    },
    "target-dir": "Debril/FeedIoBundle",
    "config": {
        "bin-dir": "bin"
    }
}
